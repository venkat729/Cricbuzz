body{
	margin:0;
	padding:0;
	background: #E3E6E3;
}
#SlideShow{
	border:none;
	width:800px;
}
#SlideShow td{
	border-bottom:1px solid none;
}
#arrange1{
	float:left;
     margin-left: 270px;
     margin-top: 30px;
}
#arrange1 #news{
	background:white;
	border:none;
}
#news td{
	border-bottom:1px solid black;
}
hr{
	color:blue;
}
#news{
	width:200px;
}
#arrange{
	width:170px;
	float:left;
	margin-left: 30px;
	padding:30px;
	border:none;
	background: #60ab59;
}
#arrange table{
	border:none;
}
table{
	width:120%;
	border:1px solid black;
}
#set{
	background:blue;
}
::-moz-selection{
	background: #66c8ed;
}
::-webkit-selection{
	background: #66c8ed;
}
::-o-selection{
	background: #66c8ed;
}
::selection{
	background: #66c8ed;
}
a{
	text-decoration:none;
	color:#3D3D3D;
}
img{
	user-select:none;
	-moz-user-select:none;
	-webkit-user-select:none;
}
